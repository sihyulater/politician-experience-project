, D.C., November 10, 1907; interment in Rose Hill Cemetery, Hagerstown, Washington County, Md.", "relationship" : [{ "relationshipType" : "grandfather", "relatedTo" : { "usCongressBioId" : "B001222", "familyName" : "Byron", "givenName" : "Katharine", "middleName" : "Edgar", "unaccentedFamilyName" : "Byron", "unaccentedGivenName" : "Katharine", "unaccentedMiddleName" : "Edgar", "birthDate" : "1903", "birthCirca" : false, "deathDate" : "1976", "deathCirca" : false } }, { "relationshipType" : "great-grandfather", "relatedTo" : { "usCongressBioId" : "B001221", "familyName" : "Byron", "givenName" : "Goodloe", "middleName" : "Edgar", "unaccentedFamilyName" : "Byron", "unaccentedGivenName" : "Goodloe", "unaccentedMiddleName" : "Edgar", "birthDate" : "1929", "birthCirca" : false, "deathDate" : "1978", "deathCirca" : false } }], "jobPositions" : [{ "job" : { "name" : "Representative", "jobType" : "CongressMemberJob" }, "congressAffiliation" : { "congress" : { "name" : "The 48th United States Congress", "congressNumber" : 48, "congressType" : "USCongress", "startDate" : "1883-12-03", "endDate" : "1885-03-04" }, "partyAffiliation" : [{ "party" : { "name" : "Republican" } }], "caucusAffiliation" : [{ "party" : { "name" : "Republican" } }], "represents" : { "regionType" : "StateRegion", "regionCode" : "MD" } } }, { "job" : { "name" : "Representative", "jobType" : "CongressMemberJob" }, "congressAffiliation" : { "congress" : { "name" : "The 49th United States Congress", "congressNumber" : 49, "congressType" : "USCongress", "startDate" : "1885-03-04", "endDate" : "1887-03-03" }, "partyAffiliation" : [{ "party" : { "name" : "Republican" } }], "caucusAffiliation" : [{ "party" : { "name" : "Republican" } }], "represents" : { "regionType" : "StateRegion", "regionCode" : "MD" } } }, { "job" : { "name" : "Representative", "jobType" : "CongressMemberJob" }, "startDate" : "1887-03-04", "startCirca" : false, "congressAffiliation" : { "congress" : { "name" : "The 50th United States Congress