{ "usCongressBioId" : "M000739", "familyName" : "Miller", "givenName" : "John", "middleName" : "Franklin", "honorificPrefix" : "Mr.", "unaccentedFamilyName" : "Miller", "unaccentedGivenName" : "John", "unaccentedMiddleName" : "Franklin", "birthDate" : "1862-06-09", "birthCirca" : false, "deathDate" : "1936-05-28", "deathCirca" : false, "image" : [{ "contentUrl" : "https://history.house.gov/Collection/Detail/25769818907", "caption" : "Collection of the U.S. House of Representatives <a href=\"https://history.house.gov/Collection/Detail/25769818907\" target=\"_blank\" rel=\"noopener\">About this object</a>" }], "profileText" : "A Representative from Washington; born on a farm near South Bend, St. Joseph County, Ind., June 9, 1862; attended the public schools; was graduated from the law department of Valparaiso (Ind.) University in 1887 and was admitted to the bar the same year; moved to Seattle, Wash., in 1888 and commenced the practice of law; prosecuting attorney of King County 1890-1894; deputy prosecuting attorney 1905-1908; mayor of Seattle 1908-1910; elected as a Republican to the Sixty-fifth and to the six succeeding Congresses (March 4, 1917-March 3, 1931); unsuccessful candidate for renomination in 1930; was a member of the congressional delegation which, with the Secretary of War, visited the American forces in France and Germany in 1919; resumed the practice of law; died in Seattle, Wash., May 28, 1936; interment in Acacia Mausoleum.", "relationship" : [{ "relationshipType" : "nephew", "relatedTo" : { "usCongressBioId" : "M000740", "familyName" : "Miller", "givenName" : "John", "middleName" : "Franklin", "unaccentedFamilyName" : "Miller", "unaccentedGivenName" : "John", "unaccentedMiddleName" : "Franklin", "birthDate" : "1831", "birthCirca" : false, "deathDate" : "1886", "deathCirca" : false } }], "jobPositions" : [{ "job" : { "name" : "Representative", "jobType" : "CongressMemberJob" }, "startDate" : "1921-03-04", "startCirca" : false, "congressAffiliation"