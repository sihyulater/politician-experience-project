onal Accommodations for the Library (Sixtieth Congress); returned to his home near Biloxi, Harrison County, Miss., and died there September 18, 1912; interment in the family vault at Carrollton, Carroll County, Miss.","relationship":[{"relationshipType":"cousin","relatedTo":{"usCongressBioId":"V000070","familyName":"Vardaman","givenName":"James","middleName":"Kimble","unaccentedFamilyName":"Vardaman","unaccentedGivenName":"James","unaccentedMiddleName":"Kimble","birthDate":"1861","birthCirca":false,"deathDate":"1930","deathCirca":false}}],"jobPositions":[{"job":{"name":"Representative","jobType":"CongressMemberJob"},"congressAffiliation":{"congress":{"name":"The 44th United States Congress","congressNumber":44,"congressType":"USCongress","startDate":"1875-03-05","endDate":"1877-03-04"},"partyAffiliation":[{"party":{"name":"Democrat"}}],"caucusAffiliation":[{"party":{"name":"Democrat"}}],"represents":{"regionType":"StateRegion","regionCode":"MS"}}},{"job":{"name":"Representative","jobType":"CongressMemberJob"},"congressAffiliation":{"congress":{"name":"The 45th United States Congress","congressNumber":45,"congressType":"USCongress","startDate":"1877-03-05","endDate":"1879-03-03"},"partyAffiliation":[{"party":{"name":"Democrat"}}],"caucusAffiliation":[{"party":{"name":"Democrat"}}],"represents":{"regionType":"StateRegion","regionCode":"MS"}}},{"job":{"name":"Representative","jobType":"CongressMemberJob"},"congressAffiliation":{"congress":{"name":"The 46th United States Congress","congressNumber":46,"congressType":"USCongress","startDate":"1879-03-18","endDate":"1881-03-03"},"partyAffiliation":[{"party":{"name":"Democrat"}}],"caucusAffiliation":[{"party":{"name":"Democrat"}}],"represents":{"regionType":"StateRegion","regionCode":"MS"}}},{"job":{"name":"Representative","jobType":"CongressMemberJob"},"congressAffiliation":{"congress":{"name":"The 47th United States Congress","congressNumber":47,"congressType":"USCongress","startDate":"1881-03-04","endDate":"1883-03-03"},