ecord" : [{ "name" : "ca. 1909-1929, 25 cubic feet.", "recordType" : ["Papers"], "description" : "The papers of William Walton Griest contain business and personal correspondence relating to politics, education, immigration, roads and waterways, railroads, economic issues, agriculture, trade and commerce, taxes, the Postal Service, the Susquehanna Iron Company, the Susquehanna Bridge, and many other topics. There are also congressional bills and speeches, financial information for the businesses he was involved with, and papers reflecting his efforts to improve Lancaster County's road system and to survey the county's waterways for expanded uses. A finding aid is available in the repository and online.", "recordLocation" : { "name" : "Lancaster County Historical Society", "location" : { "addressLocality" : "Lancaster", "addressRegion" : "PA" } } }] }
