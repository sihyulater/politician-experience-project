gressNumber":65,"congressType":"USCongress","startDate":"1917-03-05","endDate":"1919-03-04"},"partyAffiliation":[{"party":{"name":"Democrat"}}],"caucusAffiliation":[{"party":{"name":"Democrat"}}],"represents":{"regionType":"DistrictRegion","regionCode":"MO"}}},{"job":{"name":"Representative","jobType":"CongressMemberJob"},"startDate":"1927-03-04","startCirca":false,"congressAffiliation":{"congress":{"name":"The 70th United States Congress","congressNumber":70,"congressType":"USCongress","startDate":"1927-12-05","endDate":"1929-03-04"},"partyAffiliation":[{"party":{"name":"Democrat"}}],"caucusAffiliation":[{"party":{"name":"Democrat"}}],"represents":{"regionType":"DistrictRegion","regionCode":"MO"}}},{"job":{"name":"Representative","jobType":"CongressMemberJob"},"startDate":"1923-03-04","startCirca":false,"congressAffiliation":{"congress":{"name":"The 68th United States Congress","congressNumber":68,"congressType":"USCongress","startDate":"1923-12-03","endDate":"1925-03-04"},"partyAffiliation":[{"party":{"name":"Democrat"}}],"caucusAffiliation":[{"party":{"name":"Democrat"}}],"represents":{"regionType":"DistrictRegion","regionCode":"MO"}}}],"creativeWork":[],"researchRecord":[{"name":"1890-1963, 16.8 linear feet.","recordType":["Papers"],"description":"The papers of Milton A. Romjue contain constituent, patronage, political, campaign, legislative, administrative, and personal files that document north-central and northeastern sections of the state from 1916 to 1942. The bulk of the material is correspondence, but the papers also include newspaper clippings, campaign literature, government publications, and photographs. A finding aid is available in the repository and online.","recordLocation":{"name":"Joint Collection University of Missouri Western Historical Manuscript Collection-Columbia and State Historical Society of Missouri Manuscripts","location":{"addressLocality":"Columbia","addressRegion":"MO"}}}]}
